{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
